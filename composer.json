{
    "name": "czim/laravel-service",
    "description": "Basic webservice framework for Laravel.",
    "keywords": [
        "service",
        "webservice",
        "rest",
        "soap"
    ],
    "homepage": "https://github.com/czim/laravel-service",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pxlwidgets.com",
            "homepage": "https://github.com/czim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "czim/laravel-dataobject": "^1.0|^2.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Service\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Service\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
