{
    "name": "czim/laravel-service",
    "description": "Basic webservice framework for Laravel.",
    "keywords": [
        "service",
        "webservice",
        "rest",
        "soap"
    ],
    "homepage": "https://github.com/czim/laravel-service",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pxlwidgets.com",
            "homepage": "http://www.pxlwidgets.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "czim/laravel-dataobject": "^2.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/support": "^5|^6|^7|^8"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Service\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Service\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Czim\\Service\\Providers\\ServiceServiceProvider"
            ]
        }
    }
}
