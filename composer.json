{
    "name": "czim/laravel-service",
    "description": "Basic webservice framework for Laravel.",
    "keywords": [
        "service",
        "webservice",
        "rest",
        "soap"
    ],
    "homepage": "https://github.com/czim/laravel-service",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen.zimmerman@endeavour.nl",
            "homepage": "https://endeavour.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "czim/laravel-dataobject": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "nunomaduro/larastan": "^2.4",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^9.5.10",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Service\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Service\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Czim\\Service\\Providers\\ServiceServiceProvider"
            ]
        }
    }
}
